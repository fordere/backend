<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <add key="Debug" value="${Debug}"/>

    <add key="API.Url" value="${API.Url}" />

    <add key="Smtp.Enabled" value="${Smtp.Enabled}" />
    <add key="Smtp.Host" value="${Smtp.Host}" />
    <add key="Smtp.Port" value="${Smtp.Port}" />
    <add key="Smtp.Sender" value="${Smtp.Sender}" />

    <add key="DB.Host" value="${DB.Host}" />
    <add key="DB.Name" value="${DB.Name}" />
    <add key="DB.User" value="${DB.User}" />
    <add key="DB.Pass" value="${DB.Pass}" />

    <add key="CORS.Enabled" value="${CORS.Enabled}" />
    <!-- [if ${CORS.Enabled}] -->
    <add key="CORS.AllowedOrigins" value="${CORS.AllowedOrigins}" />
    <!-- [endif] -->
  </appSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
</configuration>